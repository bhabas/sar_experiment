<?xml version="1.0"?>
<launch>
  <include file="$(find sar_launch)/Load_Params.launch"/>
  <include file="$(find sar_launch_exp)/Load_Params.launch"/>


  <rosparam command="load" file="$(find sar_config_exp)/crazyflieTypes.yaml" />
  <rosparam command="load" file="$(find sar_config_exp)/crazyflies.yaml" />

  <node pkg="crazyswarm" type="crazyswarm_server" name="crazyswarm_server" output="screen" >
    <rosparam>
      # Logging configuration (Use enable_logging to actually enable logging)
      genericLogTopics: ["States_B_O","States_B_P","TrgState"]
      genericLogTopicFrequencies: [100,100,500]


      <!-- Important States -->
      <!-- SIZE: (24/26) BYTES -->
      genericLogTopic_States_B_O_Variables: [
      "Z_States.r_BOxy","Z_States.r_BOz",
      "Z_States.V_BOxy","Z_States.V_BOz",
      "Z_States.Quat_BO",
      "Z_States.Omega_BOxy","Z_States.Omega_BOz",
      "Z_States.dOmega_BOy",
      ]

      <!-- SIZE: (0/26) BYTES -->
      genericLogTopic_States_B_P_Variables: [
      "Z_States.r_PBxy","Z_States.r_PBz",
      "Z_States.VelRel_BP",
      "Z_States.D_perp","Z_States.Tau",
      "Z_States.Theta_x",
      "Z_States.Pol_Actions",
      "Z_Trg.Trg_Flag",
      ]

      <!-- SIZE: (24/26) BYTES -->
      <!-- genericLogTopic_CTRL_Output_Variables: [
      "Z_States.Mxy",      "Z_States.FMz",
      "Z_States.f_12",     "Z_States.f_34",
      "Z_States.PWM12",    "Z_States.PWM34",
      ] -->

      <!-- SIZE: (18/26) BYTES -->
      <!-- genericLogTopic_SetPoints_Variables: [      
      "Z_SetPoints.xy",    "Z_SetPoints.z",
      "Z_SetPoints.vxy",   "Z_SetPoints.vz",
      "Z_SetPoints.axy",   "Z_SetPoints.az",
      ] -->

      <!-- SIZE: (26/26) BYTES -->
      genericLogTopic_TrgState_Variables: [
      "Z_Trg.D_perp",         "Z_Trg.Tau",
      "Z_Trg.Theta_x",        "Z_Trg.VelRel_BP",
      "Z_Trg.Quat_BO",        "Z_Trg.Omega_BOy", 
      "Z_Trg.Pol_Actions",
      ]

      <!-- SIZE: (14/26) BYTES -->
      <!-- genericLogTopic_Flags_Variables: [
      "CTRL_Flags.Pos_Ctrl",        "CTRL_Flags.Vel_Ctrl",
      "CTRL_Flags.Motorstop",       "CTRL_Flags.Moment_Flag",
      "CTRL_Flags.Tumbled",         "CTRL_Flags.Tumble_Detect",
      "CTRL_Flags.Pol_Armed",       "CTRL_Flags.CamActive",
      ] -->

      <!-- SIZE: (4/26) BYTES -->
      <!-- genericLogTopic_Misc_Variables: [
      "CTRL_Flags.CustomThrust",    "CTRL_Flags.CustomPWM",
      "CTRL_Flags.SafeModeEnable",  "CTRL_Flags.AttCtrl_Flag",
      ] -->

      
      # firmware parameters for all drones (use crazyflieTypes.yaml to set per type, or
      # allCrazyflies.yaml to set per drone)
      firmwareParams:
        commander:
          enHighLevel: 1
        stabilizer:
          estimator: 2 # 1: complementary, 2: kalman 3: UKF
          controller: 5 # 1: PID, 2: mellinger 5: OOT controller
        kalman:
          resetEstimation: 0


      motion_capture_type: "none" # one of none,vicon,optitrack,optitrack_closed_source,qualisys,vrpn
      motion_capture_host_name: "192.168.1.100:801"
      object_tracking_type: "libobjecttracker" # one of motionCapture, libobjecttracker
      send_position_only: True # True: position only; False: position+orientation 
      

      # motion_capture_interface_ip: "" # optional for optitrack with multiple interfaces
      save_point_clouds: "/dev/null" # set to a valid path to log mocap point cloud binary file.
      print_latency: True
      write_csvs: False
      force_no_cache: False
      enable_parameters: True
      enable_logging: True
      enable_logging_pose: False


    </rosparam>
  </node>

</launch>
